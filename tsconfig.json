{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "lib": ["es6"],
    "outDir": "lib",
    "rootDir": "src",
    "downlevelIteration": true,
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}